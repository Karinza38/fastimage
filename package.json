{
  "name": "fastimage",
  "version": "1.2.0",
  "description": "FastImage finds the size or type of an image given its URL by fetching as little as needed.",
  "main": "main.js",
  "files": [
    "main.js",
    "lib",
    "examples",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "postpublish": "git push origin --tags && git push origin",
    "test": "VCR_MODE=playback ./node_modules/.bin/mocha",
    "posttest": "./node_modules/.bin/eslint main.js lib/*.js test/*.js",
    "coverage": "VCR_MODE=playback ./node_modules/.bin/istanbul --report=html cover ./node_modules/.bin/_mocha",
    "travis-ci": "VCR_MODE=playback ./node_modules/.bin/istanbul --report=lcovonly cover ./node_modules/.bin/_mocha && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls.js",
    "docs": "./node_modules/.bin/jsdoc -c .jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -r"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShogunPanda/fastimage"
  },
  "keywords": [
    "fast",
    "fastimage",
    "image",
    "size",
    "dimensions",
    "resolution",
    "width",
    "height",
    "png",
    "jpeg",
    "bmp",
    "gif",
    "psd",
    "tiff",
    "webp",
    "svg"
  ],
  "author": "Shogun <shogun@cowtech.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShogunPanda/fastimage/issues"
  },
  "homepage": "https://sw.cowtech.it/fastimage",
  "dependencies": {
    "image-size": "~0.5.0",
    "request": "~2.69.0"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.11.8",
    "eslint": "~2.4.0",
    "ink-docstrap": "~1.1.4",
    "istanbul": "~0.4.2",
    "jsdoc": "~3.4.0",
    "mocha": "~2.4.5",
    "sinon": "~1.17.3",
    "sepia": "^2.0.1",
    "through": "^2.3.6"
  }
}
